{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"files": { "maxSize": 999999999 },
	"linter": {
		"rules": {
			"a11y": {
				"all": false
			},
			"correctness": {
				"noUnusedVariables": "error",
				"noUnusedImports": "error",
				"useJsxKeyInIterable": "off"
			},
			"style": {
				"useEnumInitializers": "off"
			},
			"suspicious": {
				"noExplicitAny": "off"
			}
		}
	}
}
